{
  "invocation": "power pie",
  "version": "1.0.0",
  "intents": {
    "DevicePowerIntent": {
      "phrases": [
        "turn light {status}"
      ],
      "entities": {
        "status": {
          "type": "DeviceStatusInputType"
        }
      }
    }
  },
  "entityTypes": {
    "DeviceStatusInputType": {
      "values": [
        {
          "id": "on",
          "value": "on",
          "synonyms": ["activate", "enable", "start"]
        },
        {
            "id": "off",
            "value": "off",
            "synonyms": ["deactivate", "terminate", "disable", "stop"]
        }
      ]
    }
  }
}
